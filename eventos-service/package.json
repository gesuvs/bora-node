{
  "name": "event-service",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "scripts": {
    "clear": "rimraf dist",
    "build": "yarn clear && babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpileOnly --ignore-watch node_modules --no-notify src/index.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js --config ormconfig.dev.js",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "^2.0.1",
    "date-fns": "^2.14.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "kafkajs": "^1.12.0",
    "pg": "^7.3.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.25"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@prisma/cli": "^2.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^8.0.29",
    "@types/redis": "^2.8.22",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.3.0",
    "jest": "^26.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "3.3.0",
    "ts-node-dev": "^1.0.0-pre.49",
    "tsconfig-paths": "^3.9.0",
    "typescript": "3.9.2",
    "youch": "^2.0.10",
    "youch-terminal": "^1.0.0"
  }
}
