{
  "name": "qrcode-service",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clear": "rimraf dist",
    "start": "node dist/index.js",
    "build": "yarn clear && tsc && cp ormconfig.prod.js ./dist",
    "dev": "ts-node-dev --respawn --transpileOnly src/index.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js --config ormconfig.prod.js",
    "typeorm:prod": "ts-node-dev ./node_modules/typeorm/cli.js --config dist/ormconfig.prod.js",
    "typeorm:dev": "ts-node-dev ./node_modules/typeorm/cli.js --config ormconfig.dev.js"
  },
  "dependencies": {
    "@marblejs/core": "^3.1.2",
    "@marblejs/middleware-body": "^3.1.2",
    "@marblejs/middleware-io": "^3.1.2",
    "@marblejs/middleware-jwt": "^3.1.2",
    "@marblejs/middleware-logger": "^3.1.2",
    "add": "^2.0.6",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "fp-ts": "^2.6.2",
    "mysql": "^2.14.1",
    "pg": "^8.2.1",
    "qrcode": "^1.4.4",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^6.5.5",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "0.2.25"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
